generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Student {
  id        Int    @id @default(autoincrement())
  firstName String
  email     String @unique
  nic       String
  address   String
  program   String
}

model Teacher {
  id       Int    @id @default(autoincrement())
  fullName String
  email    String @unique
  nic      String @unique
  address  String
  subject  String
}



